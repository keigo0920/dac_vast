<h1>キャンペーン一覧</h1>
  <table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Start</th>
        <th>End</th>
        <th>Limit</th>
        <th>URL</th>
      </tr>
    </thead>
    <tbody>
  <% @campaigns.each do |campaign| %>
    <tr>
     <td><%= link_to campaign.id, edit_campaign_path(campaign) %></td>
     <td><%= campaign.name %></td>
     <td><%= campaign.start_at %></td>
     <td><%= campaign.end_at %></td>
     <td><%= campaign.limit_start%></td>
     <td><%= campaign.movie_url%></td>
    </tr>
  <% end %>
  </tbody>
  </table>
</ul>

<%= link_to '新規キャンペーンの登録', new_campaign_path %>