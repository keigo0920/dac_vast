<h1>id: <%= @cuepoint.id %> のキューポイント編集ページ</h1>

<%= form_for(@cuepoint) do |f| %>
  <%= f.label :name, 'キューポイント' %>
  <%= f.text_field :name %>

　<div class = "vast_url">
　  <%= vast_url(@cuepoint) %>
 </div>
<%= f.submit '登録', class: "btn btn-success" %>
<%= link_to '中止', cuepoints_path,  class: "btn btn-warning" %>
<%= link_to '削除する', @cuepoint, method: :delete, data: { confirm: '本当に削除してよろしいですか？' },  class: "btn btn-danger" %>
<% end %>